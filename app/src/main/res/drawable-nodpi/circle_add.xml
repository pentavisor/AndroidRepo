<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:right="10dp"
        android:left="10dp"
        android:top="10dp"
        android:bottom="10dp">
        <shape android:shape="oval">

            <stroke
                android:color="@android:color/holo_red_light"
                android:width="3dp"
                android:dashWidth="15dp"
                android:dashGap="5dp"></stroke>
        </shape>
    </item>
    <item
        android:right="30dp"
        android:left="30dp">
        <shape android:shape="line">
            <stroke android:width="4dp" android:color="@android:color/holo_red_light" />
        </shape>

    </item>
    <item  android:right="30dp"
        android:left="30dp"
        >
        <rotate
            android:fromDegrees="90"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="-90">
            <shape android:shape="line">
                <stroke android:width="4dp"  android:color="@android:color/holo_red_light" />
            </shape>

        </rotate>
    </item>
</layer-list>